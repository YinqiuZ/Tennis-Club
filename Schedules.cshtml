@model IEnumerable<alpha3.Models.Schedule>

@{
    ViewData["Title"] = "Upcoming Schedules";
}

<h2>@ViewData["Title"]</h2>

<ul>
    @foreach (var schedule in Model)
    {
        <li>@Html.DisplayFor(modelItem => schedule.Date): @Html.DisplayFor(modelItem => schedule.Description)</li>
    }
</ul>
